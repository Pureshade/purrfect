from aiogram import Bot, Dispatcher, types
from aiogram.utils import executor
import os

# Берём токен из переменных окружения
BOT_TOKEN = os.getenv("8562345963:AAHbstzS7nvUIKaHhDSmI0txZJIJ7-Z3QJs")

bot = Bot(token=8562345963:AAHbstzS7nvUIKaHhDSmI0txZJIJ7-Z3QJs)
dp = Dispatcher(bot)

# Обработчик команды /start
@dp.message_handler(commands=['start'])
async def start(message: types.Message):
    await message.reply("Привет! Это тестовый бот для проверки работы.")

if __name__ == '__main__':
    executor.start_polling(dp, skip_updates=True)
